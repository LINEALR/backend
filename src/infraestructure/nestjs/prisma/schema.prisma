generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model t_accesorios {
  id_dispositivos      Int    @id @default(autoincrement()) @db.UnsignedInt
  dispositivo          String @db.VarChar(255)
  descripcion_sistemas String @db.VarChar(255)
  marca                String @db.VarChar(255)
  modelo               String @db.VarChar(255)
  num_serie            String @db.VarChar(255)
  id_visual            String @db.VarChar(255)
  num_sap              Int
  status               String @db.VarChar(255)
  extras               String @db.VarChar(255)
  id_area              Int
  num_control          Int    @unique(map: "t_accesorios_num_control_unique")
  codigo_propietario   Int    @unique(map: "t_accesorios_codigo_propietario_unique")
  codigo_ubicacion     Int    @unique(map: "t_accesorios_codigo_ubicacion_unique")
  id_factura           Int    @unique(map: "t_accesorios_id_factura_unique")
}

model t_areas_sistemas {
  id_area          Int    @id @default(autoincrement()) @db.UnsignedInt
  ceco_sap         String @unique(map: "t_areas_sistemas_ceco_sap_unique") @db.VarChar(255)
  descripcion_ceco String @db.VarChar(255)
  area_sistemas    Int
}

model t_articulo_sistemas {
  id_articulo     Int    @id @default(autoincrement()) @db.UnsignedInt
  codigo_articulo Int    @unique(map: "t_articulo_sistemas_codigo_articulo_unique")
  descripcion     String @db.VarChar(255)
  identificador   String @db.VarChar(255)
  consecutivo     Int
}

model t_colaboradores {
  id_colaboradores Int    @id @default(autoincrement()) @db.UnsignedInt
  num_control      Int    @unique(map: "t_colaboradores_num_control_unique")
  nombre           String @db.VarChar(255)
  correo           String @default("N/A") @db.VarChar(255)
  id_area          Int
}

model t_consumibles {
  id_dispositivos      Int    @id @default(autoincrement()) @db.UnsignedInt
  dispositivo          String @db.VarChar(255)
  descripcion_sistemas String @db.VarChar(255)
  marca                String @db.VarChar(255)
  modelo               String @db.VarChar(255)
  num_serie            String @db.VarChar(255)
  id_visual            String @db.VarChar(255)
  num_sap              Int
  status               String @db.VarChar(255)
  extras               String @db.VarChar(255)
  id_area              Int
  num_control          Int    @unique(map: "t_consumibles_num_control_unique")
  codigo_propietario   Int    @unique(map: "t_consumibles_codigo_propietario_unique")
  codigo_ubicacion     Int    @unique(map: "t_consumibles_codigo_ubicacion_unique")
  id_factura           Int    @unique(map: "t_consumibles_id_factura_unique")
}

model t_dispositivos {
  id_dispositivos      Int    @id @default(autoincrement()) @db.UnsignedInt
  dispositivo          String @db.VarChar(255)
  descripcion_sistemas String @db.VarChar(255)
  marca                String @db.VarChar(255)
  modelo               String @db.VarChar(255)
  num_serie            String @db.VarChar(255)
  id_visual            String @db.VarChar(255)
  num_sap              Int
  status               String @db.VarChar(255)
  extras               String @db.VarChar(255)
  id_area              Int
  num_control          Int    @unique(map: "t_dispositivos_num_control_unique")
  codigo_propietario   Int    @unique(map: "t_dispositivos_codigo_propietario_unique")
  codigo_ubicacion     Int    @unique(map: "t_dispositivos_codigo_ubicacion_unique")
  id_factura           Int    @unique(map: "t_dispositivos_id_factura_unique")
}

model t_factura {
  id_factura         Int           @id @default(autoincrement()) @db.UnsignedInt
  codigo_propietario Int           @unique(map: "t_factura_codigo_propietario_unique")
  proveedor_alias    String        @db.VarChar(255)
  razon_social       String        @db.VarChar(255)
  folio_factura      String        @db.VarChar(255)
  fact_so            String        @db.VarChar(255)
  fecha_factura      DateTime      @db.Date
  moneda             String        @db.VarChar(255)
  unit_s_iva         Float
  t_propietario      t_propietario @relation(fields: [codigo_propietario], references: [codigo_propietario], onUpdate: Restrict, map: "t_factura_codigo_propietario_foreign")
}

model t_historial_articulo {
  id_historial       Int      @id @default(autoincrement()) @db.UnsignedInt
  id_usuario         Int      @unique(map: "t_historial_articulo_id_usuario_unique")
  fecha_modificacion DateTime @db.DateTime(0)
  id_dispositivos    Int      @unique(map: "t_historial_articulo_id_dispositivos_unique")
  hostname           String   @db.VarChar(255)
  ceco_sap_ant       String   @db.VarChar(255)
  num_control_ant    Int
  id_movimiento      Int      @unique(map: "t_historial_articulo_id_movimiento_unique")
}

model t_movimientos {
  id_movimiento          Int    @id @default(autoincrement()) @db.UnsignedInt
  descripcion_movimineto String @db.VarChar(255)
}

model t_propietario {
  id_propietario     Int            @id @default(autoincrement()) @db.UnsignedInt
  codigo_propietario Int            @unique(map: "t_propietario_codigo_propietario_unique")
  nombre             String         @db.VarChar(255)
  t_factura          t_factura?
  t_refacciones      t_refacciones?
}

model t_refacciones {
  id_dispositivos      Int           @id @default(autoincrement()) @db.UnsignedInt
  dispositivo          String        @db.VarChar(255)
  descripcion_sistemas String        @db.VarChar(255)
  marca                String        @db.VarChar(255)
  modelo               String        @db.VarChar(255)
  num_serie            String        @db.VarChar(255)
  id_visual            String        @db.VarChar(255)
  num_sap              Int
  status               String        @db.VarChar(255)
  extras               String        @db.VarChar(255)
  id_area              Int
  num_control          Int           @unique(map: "t_refacciones_num_control_unique")
  codigo_propietario   Int           @unique(map: "t_refacciones_codigo_propietario_unique")
  codigo_ubicacion     Int           @unique(map: "t_refacciones_codigo_ubicacion_unique")
  id_factura           Int           @unique(map: "t_refacciones_id_factura_unique")
  t_propietario        t_propietario @relation(fields: [codigo_propietario], references: [codigo_propietario], onUpdate: Restrict, map: "t_refacciones_codigo_propietario_foreign")
}

model t_roles {
  id_rol Int    @id @default(autoincrement()) @db.UnsignedInt
  nombre String @db.VarChar(255)
}

model t_ubicacion_fisica {
  id_ubicacion     Int    @id @default(autoincrement()) @db.UnsignedInt
  codigo_ubicacion Int    @unique(map: "t_ubicacion_fisica_codigo_ubicacion_unique")
  descripcion      String @db.VarChar(255)
}

model t_usuario {
  id_usuario  Int     @id @default(autoincrement()) @db.UnsignedInt
  num_control Int     @unique(map: "t_usuario_num_control_unique")
  id_rol      Int     @unique(map: "t_usuario_id_rol_unique")
  contrasena  String  @db.VarChar(255)
  status      Boolean
}
